{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/jobs/(.*)",
      "destination": "/api/jobs.py"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}

