<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 64 64">
  <!-- Background circle -->
  <circle cx="32" cy="32" r="32" fill="#2563eb"/>
  
  <!-- Robot head outline -->
  <rect x="18" y="20" width="28" height="24" rx="4" fill="#ffffff" stroke="#1e40af" stroke-width="2"/>
  
  <!-- Robot eyes -->
  <circle cx="26" cy="30" r="3" fill="#2563eb"/>
  <circle cx="38" cy="30" r="3" fill="#2563eb"/>
  
  <!-- Robot antenna -->
  <line x1="32" y1="16" x2="32" y2="20" stroke="#ffffff" stroke-width="2" stroke-linecap="round"/>
  <circle cx="32" cy="14" r="2" fill="#fbbf24"/>
  
  <!-- Robot mouth (smile) -->
  <path d="M 24 38 Q 32 42 40 38" stroke="#2563eb" stroke-width="2" fill="none" stroke-linecap="round"/>
  
  <!-- Arms -->
  <rect x="12" y="28" width="6" height="3" rx="1.5" fill="#ffffff"/>
  <rect x="46" y="28" width="6" height="3" rx="1.5" fill="#ffffff"/>
  
  <!-- Body indicator lines -->
  <line x1="28" y1="44" x2="28" y2="48" stroke="#ffffff" stroke-width="2" stroke-linecap="round"/>
  <line x1="32" y1="44" x2="32" y2="48" stroke="#ffffff" stroke-width="2" stroke-linecap="round"/>
  <line x1="36" y1="44" x2="36" y1="48" stroke="#ffffff" stroke-width="2" stroke-linecap="round"/>
</svg>

